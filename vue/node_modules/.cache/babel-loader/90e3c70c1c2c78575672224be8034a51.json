AAA;AAAwC,OAAzD,E;0BAA0C;AAAA,iBAAG,C,WAAA,CAAH;AAAA,S;;;;OAA1C,CAEK,EADL,YAAA,CAAuD,oBAAvD,EAAuD;AAA5C,QAAA,IAAI,EAAC,SAAuC;AAA5B,QAAA,OAAK,EAAE,QAAA,CAAA;AAAqB,OAAvD,E;0BAAwC;AAAA,iBAAG,C,WAAA,CAAH;AAAA,S;;;;OAAxC,E;;AAAA,Q,WAAA,CACK,CAHP,CAGO,CAHP;AAAA,KADa,C;sBARjB;AAAA,aAOU,CAPV,YAAA,CAOU,kBAPV,EAOU;AAPA,QAAA,KAAK,EAAE,KAAA,CAAA,IAOP;AAPa,uBAAY;AAOzB,OAPV,E;0BACE;AAAA,iBAEe,CAFf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAAwB;8BACtB;AAAA,qBAA4D,CAA5D,YAAA,CAA4D,mBAA5D,EAA4D;4BAAzC,KAAA,CAAA,IAAA,CAAK,IAAoC;;yBAAzC,KAAA,CAAA,IAAA,CAAK,IAAL,GAAS,M;kBAAgC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAA5D,E,IAAA,E;;AAAA,gB,cAAA,CAA4D,CAA5D;AAAA,a,CADsB;;;;AAAA,WAAxB,CAEe,EACf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAAwB;8BACtB;AAAA,qBAA+D,CAA/D,YAAA,CAA+D,mBAA/D,EAA+D;4BAA5C,KAAA,CAAA,IAAA,CAAK,OAAuC;;yBAA5C,KAAA,CAAA,IAAA,CAAK,OAAL,GAAY,M;kBAAgC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAA/D,E,IAAA,E;;AAAA,gB,cAAA,CAA+D,CAA/D;AAAA,a,CADsB;;;;AAAA,WAAxB,CADe,CAFf;AAAA,S;;;;OADF,E;;AAAA,Q,SAAA,CAOU,CAPV;AAAA,K;;;;GADF,E;;AAAA,I,cAAA,CAiBI,CArFN,C","sourcesContent":["<template>\n  <div style=\"padding: 10px\">\n    <!--    功能区域-->\n    <div style=\"margin: 10px 0\">\n      <el-divider content-position=\"left\">功能区域</el-divider>\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\n    </div>\n\n    <!--    搜索区域-->\n    <div style=\"margin: 10px 0\">\n      <el-divider content-position=\"left\">查询条件</el-divider>\n      <span>角色名：</span><el-input v-model=\"search\" placeholder=\"请输入角色名\" style=\"width: 20%\" clearable></el-input>\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\">查询<i class=\"el-icon-search el-icon--right\"></i></el-button>\n    </div>\n\n    <el-divider content-position=\"left\">角色列表</el-divider>\n    <el-table\n        v-loading=\"loading\"\n        :data=\"tableData\"\n        border\n        stripe\n        style=\"width: 100%\">\n      <el-table-column\n          prop=\"id\"\n          label=\"ID\"\n          sortable\n          width=\"80\"\n      >\n      </el-table-column>\n      <el-table-column\n          prop=\"name\"\n          label=\"名称\">\n      </el-table-column>\n      <el-table-column\n          prop=\"comment\"\n          label=\"备注\">\n      </el-table-column>\n      <el-table-column label=\"权限菜单\">\n        <template #default=\"scope\">\n          <el-select clearable v-model=\"scope.row.permissions\" multiple placeholder=\"请选择\" style=\"width: 80%\">\n            <el-option v-for=\"item in permissions\" :key=\"item.id\" :label=\"item.comment\" :value=\"item.id\"></el-option>\n          </el-select>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\">\n        <template #default=\"scope\">\n          <el-button size=\"mini\" type=\"primary\" @click=\"handleChange(scope.row)\">保存权限菜单</el-button>\n          <el-button size=\"mini\" @click=\"handleEdit(scope.row)\">编辑</el-button>\n          <el-popconfirm title=\"确定删除吗？\" @confirm=\"handleDelete(scope.row.id)\">\n            <template #reference>\n              <el-button size=\"mini\" type=\"danger\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"currentPage\"\n          :page-sizes=\"[5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n\n    <el-dialog title=\"角色信息编辑\" v-model=\"dialogVisible\" width=\"30%\">\n      <el-form :model=\"form\" label-width=\"120px\">\n        <el-form-item label=\"名称\">\n          <el-input v-model=\"form.name\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"备注\">\n          <el-input v-model=\"form.comment\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n      </el-form>\n      <template #footer>\n          <span class=\"dialog-footer\">\n            <el-button @click=\"dialogVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n          </span>\n      </template>\n    </el-dialog>\n\n  </div>\n</template>\n\n<script>\n\nimport request from \"@/utils/request\";\n\nexport default {\n  name: 'Role',\n  components: {},\n  data() {\n    return {\n      loading: true,\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      permissions: []\n    }\n  },\n  created() {\n    this.load()\n  },\n  methods: {\n    handleChange(row) {\n      request.put(\"/role/changePermission\", row).then(res => {\n        if (res.code === '0') {\n          this.$message.success(\"更新成功\")\n          if (res.data) {\n            this.$router.push(\"/login\")\n          }\n        }\n      })\n    },\n    load() {\n      this.loading = true\n      request.get(\"/role\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.loading = false\n        this.tableData = res.data.records\n        this.total = res.data.total\n      })\n\n      request.get(\"/permission/all\").then(res => {\n        this.permissions = res.data\n      })\n    },\n    add() {\n      this.dialogVisible = true\n      this.form = {}\n    },\n    save() {\n\n      if (this.form.id) {  // 更新\n        request.put(\"/role\", this.form).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      } else {  // 新增\n        let userStr = sessionStorage.getItem(\"user\") || \"{}\"\n        let user = JSON.parse(userStr)\n        this.form.author = user.nickName\n\n        request.post(\"/role\", this.form).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      }\n\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row))\n      this.dialogVisible = true\n    },\n    handleDelete(id) {\n      console.log(id)\n      request.delete(\"/role/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n        this.load()  // 删除之后重新加载表格的数据\n      })\n    },\n    handleSizeChange(pageSize) {   // 改变当前每页的个数触发\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {  // 改变当前页码触发\n      this.currentPage = pageNum\n      this.load()\n    }\n  }\n}\n</script>\n"],"sourceRoot":""}]}