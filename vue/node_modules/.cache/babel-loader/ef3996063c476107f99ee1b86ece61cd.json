自动生成y轴\n            yAxis:{axisLabel: {\n                    formatter: '{value}'\n                  }},\n            //数据-data是最终要显示的数据\n            series:[{\n                name:'收入',\n                type:'line',\n                data:[],\n            }]\n            };\n        request.get(\"/order/count\").then(res => {\n          if (res.code === '0') {\n            res.data.forEach(item => {\n              option.series[0].data.push(item.count);\n              option.xAxis.data.push(item.name)\n            })\n            // 绘制图表\n            myChart.setOption(option);\n          }\n        })\n      }\n    },\n    mounted() {\n      this.drawCharts();\n    },\n  };\n    \n</script>\n"],"sourceRoot":""}]}