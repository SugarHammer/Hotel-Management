B,CAPe,EAYf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAAwB;8BACtB;AAAA,qBAA+E,CAA/E,YAAA,CAA+E,mBAA/E,EAA+E;AAArE,gBAAA,IAAI,EAAC,UAAgE;4BAA5C,KAAA,CAAA,IAAA,CAAK,OAAuC;;yBAA5C,KAAA,CAAA,IAAA,CAAK,OAAL,GAAY,M;kBAAgC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAA/E,E,IAAA,E;;AAAA,gB,cAAA,CAA+E,CAA/E;AAAA,a,CADsB;;;;AAAA,WAAxB,CAZe,EAef,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAA0B;8BACxB;AAAA,qBAA+D,CAA/D,YAAA,CAA+D,mBAA/D,EAA+D;4BAA5C,KAAA,CAAA,IAAA,CAAK,OAAuC;;yBAA5C,KAAA,CAAA,IAAA,CAAK,OAAL,GAAY,M;kBAAgC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAA/D,E,IAAA,E;;AAAA,gB,cAAA,CAA+D,CAA/D;AAAA,a,CADwB;;;;AAAA,WAA1B,CAfe,CAFf;AAAA,S;;;;OADF,E;;AAAA,Q,SAAA,CAqBU,CArBV;AAAA,K;;;;GADF,E;;AAAA,I,cAAA,C,qCA+BI,CA5IN,C","sourcesContent":["<template>\n  <div style=\"padding: 10px\">\n\n    <!--    功能区域-->\n    <div style=\"margin: 10px 0\">\n      <el-divider content-position=\"left\">功能区域</el-divider>\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\n      <el-upload\n        action=\"http://localhost:9090/user/import\"\n        :on-success=\"handleUploadSuccess\"\n        :show-file-list=false\n        :limit=\"1\"\n        accept='.xlsx'\n        style=\"display: inline-block; margin: 0 10px\"\n      >\n        <el-button type=\"primary\">导入</el-button>\n      </el-upload>\n      <el-button type=\"primary\" @click=\"exportUser\">导出</el-button>\n    </div>\n\n    <!--    搜索区域-->\n    <div style=\"margin: 10px 0\">\n      <el-divider content-position=\"left\">查询条件</el-divider>\n      <span>用户名：</span><el-input v-model=\"search\" placeholder=\"请输入用户名\" style=\"width: 30%\" clearable></el-input>\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\">查询<i class=\"el-icon-search el-icon--right\"></i></el-button>\n    </div>\n    \n    <el-divider content-position=\"left\">用户列表</el-divider>\n    <el-table\n        v-loading=\"loading\"\n        :data=\"tableData\"\n        border\n        stripe\n        style=\"width: 100%\">\n      <el-table-column\n          prop=\"id\"\n          label=\"ID\"\n          sortable\n      >\n      </el-table-column>\n      <el-table-column\n          prop=\"username\"\n          label=\"用户名\">\n      </el-table-column>\n      <el-table-column\n          prop=\"truename\"\n          label=\"姓名\">\n      </el-table-column>\n      <el-table-column\n          prop=\"telephone\"\n          label=\"电话\">\n      </el-table-column>\n      <el-table-column\n          prop=\"nickname\"\n          label=\"昵称\">\n      </el-table-column>\n      <el-table-column\n          prop=\"age\"\n          label=\"年龄\">\n      </el-table-column>\n      <el-table-column\n          prop=\"sex\"\n          label=\"性别\">\n      </el-table-column>\n      <el-table-column\n          prop=\"address\"\n          label=\"地址\">\n      </el-table-column>\n      <el-table-column label=\"角色列表\" width=\"200\" v-if=\"userform.roles[0] === 1\">\n        <template #default=\"scope\">\n          <el-select v-model=\"scope.row.roles\" multiple placeholder=\"请选择\" style=\"width: 80%\">\n            <el-option v-for=\"item in roles\" :key=\"item.id\" :label=\"item.comment\" :value=\"item.id\"></el-option>\n          </el-select>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" width=\"300\" v-if=\"userform.roles[0] === 1\">\n        <template #default=\"scope\">\n          <el-button size=\"mini\" type=\"primary\" @click=\"handleChange(scope.row)\">保存角色信息</el-button>\n          <el-button size=\"mini\" type=\"primary\" plain @click=\"handleEdit(scope.row)\">编辑</el-button>\n          <el-popconfirm title=\"确定删除吗？\" @confirm=\"handleDelete(scope.row.id)\">\n            <template #reference>\n              <el-button size=\"mini\" type=\"danger\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" v-if=\"userform.roles[0] === 4\">\n        <template #default=\"scope\">\n          <el-popconfirm title=\"确定解约该用户吗？\" @confirm=\"handleLeave(scope.row.id)\">\n            <template #reference>\n              <el-button size=\"mini\" type=\"danger\">解约</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n  </el-table>\n\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"currentPage\"\n          :page-sizes=\"[5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n\n\n    <el-dialog title=\"用户信息编辑\" v-model=\"dialogVisible\" width=\"30%\" v-if=\"userform.roles[0] === 1\">\n      <el-form :model=\"form\" label-width=\"120px\">\n        <el-form-item label=\"用户名\">\n          <el-input v-model=\"form.username\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"昵称\">\n          <el-input v-model=\"form.nickname\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"年龄\">\n          <el-input v-model=\"form.age\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"性别\">\n          <el-radio v-model=\"form.sex\" label=\"男\">男</el-radio>\n          <el-radio v-model=\"form.sex\" label=\"女\">女</el-radio>\n          <el-radio v-model=\"form.sex\" label=\"未知\">未知</el-radio>\n        </el-form-item>\n        <el-form-item label=\"地址\">\n          <el-input type=\"textarea\" v-model=\"form.address\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"账户余额\">\n          <el-input v-model=\"form.account\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n      </el-form>\n      <template #footer>\n          <span class=\"dialog-footer\">\n            <el-button @click=\"dialogVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n          </span>\n      </template>\n    </el-dialog>\n\n  </div>\n</template>\n\n<script>\n\n\nimport request from \"@/utils/request\";\n\nexport default {\n  name: 'User',\n  components: {},\n  data() {\n    return {\n      loading: true,\n      form: {},\n      userform: {},\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      roles: [],\n      dialogVisible : false\n    }\n  },\n  created() {\n    let str = sessionStorage.getItem(\"user\") || \"{}\"\n    this.userform = JSON.parse(str)\n    this.load()\n  },\n  methods: {\n    handleChange(row) {\n      request.put(\"/user/changeRole\", row).then(res => {\n        if (res.code === '0') {\n          this.$message.success(\"更新成功\")\n          if (res.data) {\n            this.$router.push(\"/login\")\n          }\n        }\n      })\n    },\n    load() {\n      this.loading = true\n      request.get(\"/user\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search,\n          userId: this.userform.id                                                                                                                                                                                                                                                                                                                \n        }\n      }).then(res => {\n        this.loading = false\n        this.tableData = res.data.records\n        this.total = res.data.total\n      })\n\n      request.get(\"/role/all\").then(res => {\n        this.roles = res.data\n      })\n    },\n    handleUploadSuccess(res) {\n      if (res.code === \"0\") {\n        this.$message.success(\"导入成功\")\n        this.load()\n      }\n    },\n    exportUser() {\n      location.href = \"http://\" + window.server.filesUploadUrl + \":9090/user/export\";\n      this.load();\n    },\n    add() {\n      this.dialogVisible = true\n      this.form = {}\n    },\n    save() {\n      if (this.form.id) {  // 更新\n        request.put(\"/user\", this.form).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      } else {  // 新增\n        request.post(\"/user\", this.form).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      }\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row))\n      this.dialogVisible = true\n    },\n    handleDelete(id) {\n      console.log(id)\n      request.delete(\"/user/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n        this.load()  // 删除之后重新加载表格的数据\n      })\n    },\n    handleLeave(id){\n      request.delete(\"/contract/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"解约成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n      })\n    },\n    handleSizeChange(pageSize) {   // 改变当前每页的个数触发\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {  // 改变当前页码触发\n      this.currentPage = pageNum\n      this.load()\n    }\n  }\n}\n</script>\n"],"sourceRoot":""}]}