         data: []\n            }\n          ]\n        };\n        request.get(\"/engineer/count\").then(res => {\n        if (res.code === '0') {\n          res.data.forEach(item => {\n            option.series[0].data.push({name: item.state, value: item.count})\n          })\n          // 绘制图表\n          myChart.setOption(option);\n        }\n      })\n      }\n    },\n    computed: {\n      echarts() {\n        return 'echarts' + Math.random() * 100000;\n      }\n    },\n    mounted() {\n      this.drawCharts();\n    },\n  };\n</script>\n\n<style></style>"]}]}